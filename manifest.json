{
  "manifest_version": 3,
  "name": "Clavier Arab",
  "version": "1.0",
  "description": "Converts Latin characters to Arabic characters",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon":"icon.png"
  }
  ,"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }]
}
